                                    <TextBlock Text="(経過時間 / 平均(予測)時間)" />
                                    <TextBlock Text="{Binding ProcessTime}" Margin="5,0,0,0" />
                                    <TextBlock Text="/" Margin="1,0,0,0" />
                                    <TextBlock Text="{Binding AverageTime}" Margin="1,0,0,0" />
                                </StackPanel>
                                <StackPanel Margin="0,3,0,0" Orientation="Horizontal">
                                    <TextBlock Text="(起動平均(予測)時間)" />
                                    <TextBlock Text="{Binding RunAverageTime}" Margin="5,0,0,0" />
                                </StackPanel>
                                <StackPanel Margin="0,3,0,0" Orientation="Horizontal">
                                    <TextBlock Text="(チェック平均(予測)時間)" />
                                    <TextBlock Text="{Binding CheckAverageTime}" Margin="5,0,0,0" />
                                </StackPanel>
                            </StackPanel>
