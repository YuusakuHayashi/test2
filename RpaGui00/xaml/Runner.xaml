                                <DataGrid Height="100" ItemsSource="{Binding RunRobotCommandDatas}" SelectedIndex="{Binding RunRobotCommandIndex}" CanUserAddRows="False" />
                            </ScrollViewer>
                        </Expander>
                    </StackPanel>
                    <StackPanel Margin="0,5,0,0" DockPanel.Dock="Top">
                        <Expander Header="処理状況">
                            <StackPanel>
                                <TextBlock Text="{Binding ViewController.Data.System.ReturnCode, Converter={StaticResource ReturnCodeToMessageConverter}}" />
